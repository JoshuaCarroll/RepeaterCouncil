    <ImplicitUsings>enable</ImplicitUsings>
    <UserSecretsId>a58c2956-442c-476d-be64-d107c560fc77</UserSecretsId>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.EntityFrameworkCore.Design" Version="8.0.8">
